<p id="notice"><%= notice %></p>
<div class="loginbox">
<h1> Login</h1>
	
    <%= form_for :session, url: login_path, :html => {:id => "fox"}, method: :post do |f| %>  
        <%= f.text_field :username, placeholder: 'Username' %>
        <%= f.password_field :password, placeholder: 'Password' %>
        <br>
        <%= f.submit 'Login' %>
    <% end %>
</div>